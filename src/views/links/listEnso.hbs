<div class="container p-4">
    <div class="row">
        {{#each totalprod1}}
        <div class="col-md-3">
            <div class="card text-center bg-dark text-light">
                <div class="card-body">
                    <a href="{{turno}}" target="_blank" class="text-light">
                        <h3 class="card-title text-uppercase">
                            {{enspapel}}
                            {{enspoli}}   
                            {{corte}}  
                        </h3>
                    </a>
                    <p class="m-2">{{impresion}}</p>
                    <p class="text-muted">
                        {{timeago created_at}}
                    </p>
                    <a class="btn btn-danger" href="/links/delete/{{id}}">
                        Delete Link
                    </a>
                    
                </div>
            </div>
        </div>
        
        
        {{else}}
        <div class="col-md-4 mx-auto">
            <div class="card card-body text-center bg-dark text-light">
                <p>There are not Links saved yet.</p>
                <a href="/links/addEnso" class="text-success">Create One!</a>
            </div>
        </div>
        {{/each}}

        
    </div>
</div>